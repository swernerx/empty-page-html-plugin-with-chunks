{
  "name": "init-stopnet-display",
  "version": "0.0.1",
  "description": "Display application for public transport information.",
  "main": "dist/index.js",
  "scripts": {
    "prep:webpack": "rollup --silent --format cjs src/webpack/index.js --file webpack.config.js --intro '// Generated by build:webpack'",
    "prep": "npm run prep:webpack",
    "start": "npm run prep && cross-env NODE_ENV=development webpack-serve ./webpack.config.js",
    "build": "npm run prep && rimraf dist && cross-env NODE_ENV=production webpack",
    "test": "jest",
    "storybook": "start-storybook --port 1449 --config-dir node_modules/edge-storybook/lib",
    "storybook:build": "rimraf docs/storybook && build-storybook --output-dir docs/storybook --config-dir node_modules/edge-storybook/lib",
    "publish:storybook": "npm run storybook:build && now --name 'init-stopnet-display-docs' docs/storybook",
    "publish:app": "npm run build && now --name 'init-stopnet-display' dist",
    "publish": "npm run publish:storybook && npm run publish:app"
  },
  "keywords": [],
  "author": {
    "name": "Init SE",
    "email": "info@initse.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastian-software/init-stopnet-display.git"
  },
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "app-manifest-loader": "^1.0.1",
    "babel-loader": "^8.0.0-beta.2",
    "babel-preset-edge": "^3.3.0",
    "cache-loader": "^1.2.2",
    "classnames": "^2.2.5",
    "cookiesjs": "^2.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "edge-core": "^1.0.0-alpha.2",
    "edge-jest": "^0.2.0",
    "edge-postcss": "^1.0.0-alpha.2",
    "edge-storybook": "^1.0.0-alpha.4",
    "edge-style": "^1.0.0-alpha.2",
    "eslint": "^4.19.1",
    "eslint-config-readable": "^1.11.1",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.71.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "leaflet": "^1.3.1",
    "mini-css-extract-plugin": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "postcss-loader": "^2.1.5",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-intl": "^2.4.0",
    "react-leaflet": "^1.9.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.58.2",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "stylelint-config-readable": "^1.1.0",
    "thread-loader": "^1.1.5",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.8.0",
    "webpack-cli": "^2.1.3",
    "webpack-serve": "^0.3.2",
    "webpack-stylish": "^0.1.8",
    "webpack-subresource-integrity": "^1.1.0-rc.4",
    "yaml-loader": "^0.5.0"
  }
}
